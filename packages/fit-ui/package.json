{
  "name": "@geniusmanyxh/fit-ui",
  "version": "1.1.1",
  "description": "基于vue3+vite+unocss的UI组件库",
  "files": [
    "dist/**",
    "resolver.ts"
  ],
  "type": "module",
  "private": false,
  "sideEffects": [
    "dist/*",
    "dist/**/*.css",
    "dist/**/*.scss",
    "**/*.css",
    "**/*.scss"
  ],
  "main": "dist/lib/entry.js",
  "module": "dist/es/entry.js",
  "types": "dist/entry.d.ts",
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/es/entry.js",
      "require": "./dist/lib/entry.js"
    },
    "./full": {
      "types": "./dist/full.d.ts",
      "import": "./dist/es/full.js",
      "require": "./dist/lib/full.js"
    },
    "./dist/index.css": "./dist/es/fitui.css",
    "./dist/uno.css": "./dist/es/uno.css",
    "./es/*/style": "./dist/es/src/*/style/index.js",
    "./es/*/style/css": "./dist/es/src/*/style/css.js",
    "./resolver": {
      "types": "./resolver.d.ts",
      "import": "./resolver.js"
    },
    "./style.css": "./dist/es/fitui.css",
    "./dist/*": "./dist/*"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:sourcemap": "vite build --sourcemap",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "oxlint src",
    "format": "oxfmt src",
    "prepublishOnly": "pnpm test:run && pnpm lint && pnpm build",
    "release:check": "pnpm test:run && pnpm lint && pnpm build",
    "release:dry": "pnpm release:check && pnpm publish --dry-run",
    "release": "pnpm release:check && pnpm publish --no-git-checks"
  },
  "keywords": [
    "vue3",
    "vite",
    "unocss",
    "ui",
    "fitui"
  ],
  "author": "geniusmanyxh",
  "repository": {
    "type": "git",
    "url": "https://github.com/geniusmanyxh/FitUI"
  },
  "license": "ISC",
  "devDependencies": {
    "@iconify-json/ic": "^1.2.4",
    "@iconify/json": "^2.2.432",
    "@iconify/types": "^2.0.0",
    "@jridgewell/sourcemap-codec": "^1.5.5",
    "@types/fs-extra": "^11.0.4",
    "@unocss/preset-icons": "^66.6.0",
    "@unocss/transformer-directives": "^66.6.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "5.1.3",
    "@vue/test-utils": "^2.4.6",
    "clipboard": "^2.0.11",
    "esno": "^4.8.0",
    "fs-extra": "^11.3.3",
    "happy-dom": "^20.4.0",
    "less": "^4.5.1",
    "less-loader": "^12.3.0",
    "oxfmt": "^0.27.0",
    "oxlint": "^1.42.0",
    "sass": "^1.97.3",
    "sass-embedded": "^1.97.3",
    "shiki": "^3.21.0",
    "terser": "^5.46.0",
    "tj-jstools": "^1.3.10",
    "typescript": "^5.9.3",
    "unocss": "^66.6.0",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-define-options": "^3.1.2",
    "vite": "7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^4.0.18",
    "vue": "^3.5.27",
    "vue-clipboard3": "^2.0.0"
  }
}
