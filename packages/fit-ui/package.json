{
  "name": "@geniusmanyxh/fit-ui",
  "version": "0.0.5",
  "description": "基于vue3+vite+unocss的UI组件库",
  "files": [
    "dist/**/*"
  ],
  "private": false,
  "main": "dist/lib/fit-ui.js",
  "module": "dist/es/fit-ui.js",
  "types": "dist/src/entry.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/entry.d.ts",
      "import": "./dist/es/fit-ui.js",
      "require": "./dist/lib/fit-ui.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:components",
    "build:all": "vite build",
    "build:less": "esno ./config/buildLess.ts",
    "build:components": "esno ./config/build.ts",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --fix --ext .ts,.vue src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\""
  },
  "keywords": [
    "vue3",
    "vite",
    "unocss",
    "ui",
    "fitui"
  ],
  "author": "geniusmanyxh",
  "license": "ISC",
  "devDependencies": {
    "@iconify-json/ic": "^1.1.13",
    "@iconify/json": "^2.2.100",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/preset-icons": "^0.55.0",
    "@unocss/transformer-directives": "^0.56.5",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.46.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "happy-dom": "^10.9.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "prettier": "^3.0.1",
    "sass": "^1.69.4",
    "terser": "^5.19.2",
    "typescript": "^5.0.2",
    "unocss": "^0.56.5",
    "unplugin-vue-define-options": "^1.3.15",
    "vite-plugin-dts": "^3.5.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^0.34.1",
    "vue": "^3.3.4"
  }
}
