{
  "name": "@geniusmanyxh/fit-ui",
  "version": "1.0.3",
  "description": "基于vue3+vite+unocss的UI组件库",
  "files": [
    "dist/**"
  ],
  "type": "module",
  "private": false,
  "main": "dist/lib/src/entry.js",
  "module": "dist/es/src/entry.js",
  "types": "dist/entry.d.ts",
  "exports": {
    ".": {
      "types": "./dist/entry.d.ts",
      "import": "./dist/es/src/entry.js",
      "require": "./dist/lib/src/entry.js"
    },
    "./*": [
      "./*",
      "./*.d.ts"
    ]
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint --fix --ext .ts,.vue src",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.vue\""
  },
  "keywords": [
    "vue3",
    "vite",
    "unocss",
    "ui",
    "fitui"
  ],
  "author": "geniusmanyxh",
  "repository": {
    "type": "git",
    "url": "https://github.com/geniusmanyxh/FitUI"
  },
  "license": "ISC",
  "devDependencies": {
    "@iconify-json/ic": "^1.1.13",
    "@iconify/json": "^2.2.100",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@types/fs-extra": "^11.0.4",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/preset-icons": "^65.4.3",
    "@unocss/transformer-directives": "^65.4.3",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "4.1.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/test-utils": "^2.4.1",
    "clipboard": "^2.0.11",
    "eslint": "^8.46.0",
    "eslint-formatter-pretty": "^5.0.0",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-vue": "^9.17.0",
    "esno": "^0.17.0",
    "fs-extra": "^11.1.1",
    "happy-dom": "^10.9.0",
    "less": "^4.2.0",
    "less-loader": "^11.1.3",
    "prettier": "^3.0.1",
    "sass": "^1.83.4",
    "sass-embedded": "^1.83.4",
    "shiki": "^1.26.2",
    "terser": "^5.19.2",
    "tj-jstools": "^1.3.9",
    "typescript": "^5.7.3",
    "unocss": "^65.4.3",
    "unplugin-vue-define-options": "^1.3.15",
    "vite": "5.4.14",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitest": "^0.34.1",
    "vue": "^3.4.26"
  }
}
