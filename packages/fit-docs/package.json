{
  "name": "fit-docs",
  "version": "1.0.0",
  "description": "Fit-UI组件库文档,静态站点",
  "main": "index.js",
  "scripts": {
    "copy:pages": "pnpm  exec -- cp -R ./docs/.vitepress/dist/** ./gitpages",
    "d:dev": "vitepress dev docs --env_type dev",
    "d:build:dev": "vitepress build docs --env_type dev",
    "d:build:prod": "vitepress build docs --env_type prod",
    "d:serve": "vitepress serve docs",
    "build": "pnpm run d:build:prod && pnpm run copy:pages",
    "build:dev": "pnpm run d:build:dev"

  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@geniusmanyxh/fit-ui": "workspace:^",
    "@types/markdown-it": "^13.0.5",
    "@types/markdown-it-container": "^2.0.8",
    "@types/node": "^20.8.8",
    "@types/prismjs": "^1.26.2",
    "@vitejs/plugin-vue-jsx": "3.0.1",
    "@vue/compiler-sfc": "^3.3.7",
    "@vue/runtime-core": "^3.3.7",
    "chalk": "^5.3.0",
    "consola": "^3.2.3",
    "dotenv": "^16.3.1",
    "escape-html": "^1.0.3",
    "markdown-it": "^13.0.2",
    "markdown-it-container": "^3.0.0",
    "prismjs": "^1.29.0",
    "sass": "^1.69.4",
    "shiki": "^0.14.5",
    "unocss": "^0.54.1",
    "vite": "^4.4.9",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "1.0.0-rc.24",
    "vitepress-demoblock": "^1.0.4",
    "vitepress-theme-demoblock": "^3.0.3",
    "vitest": "0.28.1",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "dependencies": {
    "clipboard-polyfill": "^4.0.1"
  }
}
