{
  "name": "fit-docs",
  "version": "1.0.0",
  "description": "Fit-UI组件库文档,静态站点",
  "main": "index.js",
  "scripts": {
    "copy:powershell": "pnpm exec powershell -Command Copy-Item -Path './docs/.vitepress/dist/**' -Destination './gitpages' -Recurse -Force",
    "copy:pages": "pnpm exec -- cp -R ./docs/.vitepress/dist/** ./gitpages",
    "dev": "pnpm run d:dev",
    "d:dev": "vitepress dev docs --port 8443 --env_type dev ",
    "d:build:dev": "vitepress build docs --env_type dev",
    "d:build:prod": "vitepress build docs --env_type prod",
    "d:serve": "vitepress serve docs",
    "d:build": "pnpm run d:build:prod && npx pnpm run copy:powershell",
    "build:dev": "pnpm run d:build:dev"
  },
  "keywords": [],
  "type": "module",
  "author": "geniusmanyxh",
  "license": "ISC",
  "devDependencies": {
    "@geniusmanyxh/fit-ui": "^1.1.1",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-container": "^4.0.0",
    "@types/node": "^25.0.10",
    "@types/prismjs": "^1.26.5",
    "@vitejs/plugin-vue-jsx": "5.1.3",
    "@vue/compiler-sfc": "^3.5.27",
    "@vue/runtime-core": "^3.5.27",
    "chalk": "^5.6.2",
    "consola": "^3.4.2",
    "dotenv": "^17.2.3",
    "escape-html": "^1.0.3",
    "markdown-it": "^14.1.0",
    "markdown-it-container": "^4.0.0",
    "prismjs": "^1.30.0",
    "sass": "^1.97.3",
    "shiki": "^3.21.0",
    "unocss": "^66.6.0",
    "vite": "^7.3.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vitepress": "1.6.4",
    "vitepress-demoblock": "^1.0.4",
    "vitepress-theme-demoblock": "^3.1.3",
    "vitest": "4.0.18",
    "vue": "^3.5.27",
    "vue-tsc": "^3.2.4"
  },
  "dependencies": {
    "clipboard-polyfill": "^4.1.1"
  }
}
