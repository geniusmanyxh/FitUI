{
  "name": "fitui",
  "version": "1.0.2",
  "description": "基于vue3+vite+unocss的UI组件库",
  "main": "index.js",
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:init": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "docs:build": "pnpm recursive run build --filter fit-docs --env_type prod",
    "docs:build:dev": "pnpm recursive run build:dev --filter fit-docs --env_type dev",
    "docs:dev": "pnpm recursive run d:dev --filter fit-docs --env_type dev",
    "ui:build": "pnpm recursive run build:all --filter fit-ui",
    "ui:dev": "pnpm recursive run dev --filter fit-ui",
    "preinstall": "node ./scripts/preinstall.js",
    "prepare": "husky install"
  },
  "keywords": [
    "fit",
    "ui",
    "vue3",
    "typescript",
    "unocss",
    "vite"
  ],
  "author": "geniusmanyxh",
  "license": "ISC",
  "dependencies": {
    "@geniusmanyxh/fit-ui": "workspace:^",
    "vite": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "17.0.2",
    "@commitlint/config-conventional": "17.0.2",
    "@types/less": "^3.0.3",
    "basic-ftp": "^5.0.3",
    "conventional-changelog-atom": "^4.0.0",
    "cpy": "^10.1.0",
    "fast-glob": "^3.3.1",
    "husky": "^8.0.3",
    "typescript": "^5.0.2",
    "vite-plugin-dts": "^3.5.2"
  }
}
